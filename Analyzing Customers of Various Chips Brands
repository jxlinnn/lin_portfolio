   "source": [
    "qty_segment1_by_brand = segment1.groupby(['BRAND']).apply(lambda x: x['PROD_QTY'].sum()/qty_segment1).rename('targetSegment')\n",
    "qty_other_by_brand = other.groupby(['BRAND']).apply(lambda x: x['PROD_QTY'].sum()/qty_other).rename('other')\n",
    "brand_proportions = pd.DataFrame(qty_segment1_by_brand).merge(qty_other_by_brand, on = 'BRAND')\n",
    "brand_proportions['affinityToBrand'] = brand_proportions['targetSegment']/brand_proportions['other']"
   ]
