    "rng = np.random.default_rng()\n",
    "df1['PRICE'] = df1['TOT_SALES']/df1['PROD_QTY']\n",
    "main_unitprice = df1[(df1['LIFESTAGE'].isin(['YOUNG SINGLES/COUPLES', 'MIDAGE SINGLES/COUPLES'])) & (df1['PREMIUM_CUSTOMER'] == 'Mainstream')]['PRICE']\n",
    "others_unitprice = df1[(df1['LIFESTAGE'].isin(['YOUNG SINGLES/COUPLES', 'MIDAGE SINGLES/COUPLES'])) & (df1['PREMIUM_CUSTOMER'] != 'Mainstream')]['PRICE']\n",
    "stats.ttest_ind(main_unitprice, others_unitprice, trim=.2, alternative = 'greater', random_state = rng)"
   ]
