    "pack_size = []\n",
    "for name in transaction_df['PROD_NAME']:\n",
    "    match = re.findall('\\d{2,3}(?=[gG])', name)\n",
    "    if match:\n",
    "        pack_size.append(match[0])\n",
    "transaction_df['PACK_SIZE'] = pd.to_numeric(pack_size)"
