   "cell_type": "code",
   "execution_count": 11,
   "id": "5c37d842-8c48-4fdb-b0db-f4953f3fb29c",
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(len(productWords)):\n",
    "    w = productWords.iloc[i]\n",
    "    if re.search('[0-9&/]', w):\n",
    "        productWords.iloc[i] = None\n",
    "productWords.dropna(inplace = True)"
   ]
