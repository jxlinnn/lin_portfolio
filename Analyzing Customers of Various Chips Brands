   "metadata": {},
   "outputs": [],
   "source": [
    "#Cleaning brand names\n",
    "transaction_df['BRAND'] = transaction_df['BRAND'].apply(lambda x: 'RRD' if x == 'RED' else x)\n",
    "transaction_df['BRAND'] = transaction_df['BRAND'].apply(lambda x: 'INFUZIONS' if x == 'INFZNS' else x)\n",
    "transaction_df['BRAND'] = transaction_df['BRAND'].apply(lambda x: 'NATURAL' if x == 'NCC' else x)\n",
    "transaction_df['BRAND'] = transaction_df['BRAND'].apply(lambda x: 'SUNBITES' if x == 'SNBTS' else x)\n",
    "transaction_df['BRAND'] = transaction_df['BRAND'].apply(lambda x: 'DORITOS' if x == 'DORITO' else x)\n",
    "transaction_df['BRAND'] = transaction_df['BRAND'].apply(lambda x: 'SMITHS' if x == 'SMITH' else x)\n",
    "transaction_df['BRAND'] = transaction_df['BRAND'].apply(lambda x: 'WOOLWORTHS' if x == 'WW' else x)\n",
    "transaction_df['BRAND'] = transaction_df['BRAND'].apply(lambda x: 'GRNWVES' if x == 'GRAIN' else x)\n"
   ]
